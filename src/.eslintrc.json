{
    "extends": "react-app",
    "rules":{
        "quotes": [2, "single", "avoid-escape"],
        "no-var": 2,
        "no-alert": 2,
        "comma-dangle": [2, "always-multiline"],
        "no-trailing-spaces": 2,
        "dot-notation": 2,
        "no-multi-spaces": 1,
        "no-redeclare": 2,
        "no-script-url": 2,
        "no-throw-literal": 2,
        "no-useless-concat": 2,
        "prefer-template": 2,
        "vars-on-top": 1,
        "indent": [1, 4, { "SwitchCase": 1 }],
        "prefer-const": 1
    }
}